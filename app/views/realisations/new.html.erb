<h1>Realisations#new</h1>
<p>Find me in app/views/realisations/new.html.erb</p>

<div class="container">
  <%= form_for @realisation do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :category %>
    <%= f.text_field :category %>
    <%= f.label :description %>
    <%= f.text_field :description %>
    <%= f.submit %>
  <% end %>
</div>

